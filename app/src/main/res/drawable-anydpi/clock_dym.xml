<?xml version="1.0" encoding="utf-8"?>
<adaptive-icon
  xmlns:android="http://schemas.android.com/apk/res/android">
    <background android:drawable="@color/bg_blue_google_dark" />
    <foreground>
        <layer-list>
            <item>
                <rotate android:drawable="@drawable/tm_clock_hour" android:fromDegrees="300.0" android:toDegrees="5300.0" android:pivotX="50.0%" android:pivotY="50.0%" />
            </item>
            <item>
                <rotate android:drawable="@drawable/tm_clock_minute" android:fromDegrees="60.0" android:toDegrees="60060.0" android:pivotX="50.0%" android:pivotY="50.0%" />
            </item>
            <item>
                <rotate android:drawable="@drawable/tm_clock_second" android:fromDegrees="180.0" android:toDegrees="6180.0" android:pivotX="50.0%" android:pivotY="50.0%" android:level="300" />
            </item>
            <item android:drawable="@drawable/tm_clock_top" />
        </layer-list>
    </foreground>
    <monochrome android:drawable="@drawable/tm_clock_v26" />
</adaptive-icon>